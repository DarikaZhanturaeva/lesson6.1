<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <gradient android:endColor="@color/yellow" android:startColor="@color/pink" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape>
            <gradient android:endColor="@color/pink" android:startColor="@color/yellow" />
        </shape>

    </item>

</selector>